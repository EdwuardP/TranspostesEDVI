<div class=" container-fluid">
    <div class="row">
        <div class="col-sm-12">
            @*BANNER*@
            <br />
            <section style="background-color: black">
                <div class="panel-heading">
                    <hgroup>
                        <h1 class="text-center" style="color: goldenrod"><b>EMPRESA DE TRANSPORTES EDVI EIRL</b></h1>
                    </hgroup>
                </div>
            </section>

            @*SLIDE*@
            <div id="mySlider" class="carousel slide">
                <ol class="carousel-indicators">
                    <li data-target="#mySlider" data-slide-to="0" class="active"></li>
                    <li data-target="#mySlider" data-slide-to="1"></li>
                    <li data-target="#mySlider" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="item active ">
                        <img alt="First slide" src="~/Images/Index1.jpg" wi />
                    </div>
                    <div class="item">
                        <img alt="Second slide" src="~/Images/Index2.jpg" />
                    </div>
                    <div class="item">
                        <img alt="second slide" src="~/Images/Index3.jpg" />
                    </div>
                </div>
                <a class="left carousel-control" href="#mySlider" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
                <a class="right carousel-control" href="#mySlider" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
            </div>
            
            <div id="small-form" class="btn-warning" style="color :white">

                @*a*@
                <h2 class="glyphicon glyphicon-check"> <b><u> Mineria Responsable:</u></b> </h2>
              
                <div class="container">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="embed-responsive embed-responsive-16by9">
                                <iframe class="embed-responsive-item" src="//www.youtube.com/embed/lFvvyAZBReg"></iframe>
                            </div>
                        </div>
                    </div>
                    </div>
                
                @*b*@
                <h2 class="glyphicon glyphicon-check  "> <b><u> Ventas:</u></b> </h2>
                <div class="container">
                    <br />
                    <h4 class="text-justify">Gran variedad de productos tecnologicos de calidad y con la garantia que ofrecemos.</h4>
                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/n.jpg" class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnCompu" Text="Computadoras" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/i.jpg " class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnImp" Text="Impresoras" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/a.jpg" class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnAcce" Text="Accesorios" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>


                </div>

                @*c*@
                <h2 class="glyphicon glyphicon-check  "> <b><u> Servicios:</u></b> </h2>
                <div class="container">
                    <br />
                    <h4 class="text-justify">Brindamos los mejores servicios de soporte para sus necesidades.</h4>
                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/sr.jpg" class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnRed" Text="Soporte en Redes" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/sh.jpg" class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnHard" Text="Soporte Hardware" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="panel-primary ">
                            <img src="Images/ss.jpg" class="col-xs-4 col-sm-4 col-md-12 col-lg-12 img-responsive img-thumbnail" alt="Ventas">
                            <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12">
                                <br>
                                <asp:Button ID="btnSoft" Text="Soporte Software" CssClass="btn btn-danger" runat="server" />
                                <br />
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>


                </div>

            </div>



        </div>
    </div>
</div>


